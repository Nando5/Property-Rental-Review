<h1>Tenants#show</h1>
<p>Find me in app/views/tenants/show.html.erb</p>

<h1><%= @tenant.name %></h1>
	<div class="myList">
		<ul>
		  <li><%= @tenant.name %></li>
		  <li><%= @tenant.email %></li>
		
		  <li><%= link_to('Edit', edit_tenant_path(@tenant)) %></li>
		  <li><%= link_to('Delete', @tenant, :method => 'delete', :data => {:confirm => 'Are you sure?'}) %></li>
		</ul>
	</div>

<h3>Property</h3>
  <% @tenant.properties.each do |property| %>
  	<%= property.street_address %>
  	<%= property.suburb %>
  	<%= property.state %>
  	<%= property.postcode %>
  <% end %>

<h3>Review</h3>
  <% @tenant.reviews.each do |review| %>
  	<%= review.comment %>
  	<%= review.rating %>
  <% end %>
