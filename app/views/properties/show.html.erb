<h1>Properties#show</h1>
<p>Find me in app/views/properties/show.html.erb</p>

<h1><%= @property.suburb %></h1>
	<div class="myList">
		<ul>
		  <li><%= @property.street_address %></li>
		  <li><%= @property.suburb %></li>
		  <li><%= @property.state %></li>
		  <li><%= @property.postcode %></li>
		
		  
		</ul>
	</div>

<h3>Review</h3>

  <% @property.reviews.each do |review| %>
  	<%= review.comment %>
  	<%= review.rating %>

<% end %>